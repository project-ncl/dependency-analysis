<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd"
    version="6">

    <application-name>Dependency-Analysis testsuite</application-name>
    <display-name>Dependency-Analysis testsuite</display-name>

    <module>
        <ejb>common.jar</ejb>
    </module>
    <module>
        <ejb>communication.jar</ejb>
    </module>
    <module>
        <ejb>source-code-manager.jar</ejb>
    </module>
    <module>
        <ejb>reports-model.jar</ejb>
    </module>
    <module>
        <ejb>reports-backend.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>reports-rest.war</web-uri>
            <context-root>testsuite</context-root>
        </web>
    </module>
    <module>
        <ejb>bc-model.jar</ejb>
    </module>
    <module>
        <ejb>bc-backend.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>bc-rest.war</web-uri>
            <context-root>testsuite-bc</context-root>
        </web>
    </module>
    <module>
        <ejb>testsuite.jar</ejb>
    </module>
    <library-directory>lib</library-directory>
</application>
